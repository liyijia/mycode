<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  namespace="LY.EMIS5.Entities.Core.Stock" assembly="LY.EMIS5.Entities">
  <class name="Storage" table="S_Storage" >
    <cache usage="read-write" />
    <id name="Id">
      <column name="Id" sql-type="int" not-null="true"/>
      <generator class="native"/>
    </id>
    <property name="No" />
    <property name="Type" />
    <property name="Kind" />
    <property name="Total" precision="20" scale="2"/>
    <property name="Payment" precision="20" scale="2"/>
    <property name="Debt" precision="20" scale="2"/>
    <many-to-one name="Creator" class="LY.EMIS5.Entities.Core.Memberships.Manager" lazy="false" column="Creator_Id"/>
    <property name="CreateDate" />
    <many-to-one name="Buyer" class="LY.EMIS5.Entities.Core.Memberships.Manager" lazy="false" column="Buyer_Id"/>
    <property name="IsInvoice" />
    <many-to-one name="Supplier" class="Supplier" lazy="false" column="Supplier_Id"/>
    <many-to-one name="Auditor" class="LY.EMIS5.Entities.Core.Memberships.Manager" lazy="false" column="Auditor_Id"/>
    <property name="AuditDate" />
    <property name="Status" />
  </class>
</hibernate-mapping>