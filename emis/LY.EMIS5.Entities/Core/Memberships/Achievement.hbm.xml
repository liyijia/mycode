<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  namespace="LY.EMIS5.Entities.Core.Memberships" assembly="LY.EMIS5.Entities">
  <class name="Achievement" table="Achievement" >
    <cache usage="read-write" />
    <id name="Id">
      <column name="Id" sql-type="int" not-null="true"/>
      <generator class="native"/>
    </id>
    <property name="ProjectName" />
    <property name="Scale" />
    <property name="StartDate" />
    <property name="EndDate" />
    <property name="ProjectManager" />
    <property name="Type" />
    <many-to-one name="Manager" class="Manager" lazy="false" column="Manager_Id"/>
    <property name="CreateDate" />
  </class>
</hibernate-mapping>