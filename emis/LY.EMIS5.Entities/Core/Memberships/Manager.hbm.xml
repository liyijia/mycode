<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  namespace="LY.EMIS5.Entities.Core.Memberships" assembly="LY.EMIS5.Entities">
  <class name="Manager" table="Manager">
    <cache usage="read-write" />
    <id name="Id">
      <column name="Id" sql-type="int" not-null="true"/>
      <generator class="native"/>
    </id>
    <property name="Kind" />
    <property name="Name" />
    <property name="Phone" />
    <property name="Sex" />
    <property name="Email" />
    <property name="UserName" />
    <component name="Password">
      <property name="Value">
        <column name="Password" sql-type="nvarchar(50)" not-null="true" />
      </property>
    </component>
    <property name="IsEnabled" />
    <property name="CreateTime" />
    <many-to-one name="Company" class="Company" lazy="false" column="Company_Id"/>
  </class>
</hibernate-mapping>