@using LY.EMIS5.Entities.Core.Memberships;
@using LY.EMIS5.Common;

@model Records
@{
    ViewBag.Title = "查看备案";
}@section Script
{

}
<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                备案信息
            </header>
            <div class="panel-body">
                <form class="form-horizontal adminex-form" method="post">
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">区域</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Area</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">是否备案</label>
                        <div class="col-sm-10">
                            <label class="control-label">@(Model.IsRecord?"是":"否")</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">网址</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.WebSite</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">登录名</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Username</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">密码</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Password</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">电话</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Phone</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">备案情况</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Situation</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">备案时间</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Date.ToYearMonthDayString()</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">备注</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Remarks</label>
                        </div>
                    </div>
                </form>
            </div>
        </section>
    </div>
</div>