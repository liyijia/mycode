@using LY.EMIS5.Entities.Core.Memberships;
@using LY.EMIS5.Common.Extensions;
@using LY.EMIS5.Common;

@model BidProject
@{
    ViewBag.Title = "中标项目";
}@section Script
{

}
<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                中标项目信息
            </header>
            <div class="panel-body">
                <form class="form-horizontal adminex-form" method="post">
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目名称</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.ProjectName</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目类型</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.ProjectType</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目类型</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.ProjectProgress.GetDescription()</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">规模</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Scale</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">中标价格(万元)</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Money</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">中标时间</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.BidDate.ToYearMonthDayString()</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目所在地</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Address</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目实施人</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.UserName</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">联系电话</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.TeletePhone</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">计划开工时间</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.OpenDate.ToYearMonthDayString()</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">计划竣工时间</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.EndDate.ToYearMonthDayString()</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">进场时间</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.InDate.ToYearMonthDayString()</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">工期</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.TimeLimit</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目经理</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.ProjectManager</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">班组成员</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Members</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目部递交分公司资料</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.Data</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">项目部配备资料</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.ProjectData</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">创建人</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.CreateManager.Name</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">创建时间</label>
                        <div class="col-sm-10">
                            <label class="control-label">@Model.CreateDate.ToChineseDateString()</label>
                        </div>
                    </div>
                </form>
            </div>
        </section>
    </div>
</div>